@page
@model MaerskLineCMS.Pages.Vessel.DetailsModel

@{
    ViewData["Title"] = "Vessel Details";
    Layout = "~/Pages/Shared/_Layout_Admin.cshtml";
}

<h1>Details</h1>

<div>
    <h4>VESSEL</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Vessel ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.VESSEL.Vessel_ID)
